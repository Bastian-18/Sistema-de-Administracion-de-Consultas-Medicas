	<queryString>
		<![CDATA[SELECT 
    c.idconsulta,
    c.fecha,
    c.hora,
    c.estado,
    c.motivo,
    c.totalcosto,
    cl.nombre as cliente_nombre,
    cl.apellido as cliente_apellido,
    cl.dni as cliente_dni,
    d.nombre as doctor_nombre,
    d.apellido as doctor_apellido,
    d.especialidad as doctor_especialidad
FROM consulta c
INNER JOIN cliente cl ON c.cliente_id = cl.idcliente
INNER JOIN doctor d ON c.doctor_id = d.iddoctor
ORDER BY c.fecha DESC, c.hora DESC]]>
	</queryString>
