	<queryString>
		<![CDATA[SELECT 
    p.idpago,
    p.fechapago,
    p.monto,
    p.metodopago,
    p.estado,
    p.numeroreferencia,
    p.notas,
    c.fecha as consulta_fecha,
    c.hora as consulta_hora,
    cl.nombre as cliente_nombre,
    cl.apellido as cliente_apellido
FROM pago p
INNER JOIN consulta c ON p.consulta_id = c.idconsulta
INNER JOIN cliente cl ON c.cliente_id = cl.idcliente
ORDER BY p.fechapago DESC]]>
